{
  "name": "angular-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-dev": "ng serve --configuration=development",
    "build": "ng build",
    "build-dev": "ng build --configuration=development",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prettier:check": "prettier --config .prettierrc --check \"src/**/*.{ts,css,html}\"",
    "prepare": "cd ../.. && husky install bp-web-client/angular-app/.husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cd ../.. && lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.js": "prettier --write --ignore-unknown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/cdk": "^15.1.1",
    "@angular/cli": "^15.1.1",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/material": "^15.1.1",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "ajv": "^8.12.0",
    "angular-google-tag-manager": "^1.7.0",
    "bootstrap": "4.3.1",
    "bytepushers-js-core": "^0.1.15",
    "bytepushers-js-obj-extensions": "0.0.13",
    "jquery": "^3.4.1",
    "ng-bootstrap-form-validation": "^9.0.0",
    "ngx-bootstrap": "^5.2.0",
    "ngx-bootstrap-slider": "^1.9.0",
    "ngx-page-scroll": "^7.0.3",
    "ngx-page-scroll-core": "^6.0.0",
    "ngx-spinner": "^10.0.1",
    "primeicons": "^5.0.0",
    "primeng": "^15.2.0",
    "rxjs": "~7.8.0",
    "ssh2-sftp-client": "^9.0.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.1",
    "@angular/cli": "~15.1.1",
    "@angular/compiler-cli": "^15.1.0",
    "@angular/language-service": "^15.1.0",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "dotenv": "^8.2.0",
    "husky": "^7.0.0",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^12.3.3",
    "prettier": "2.5.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9.4"
  }
}
